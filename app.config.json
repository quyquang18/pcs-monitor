{
  "expo": {
    "name": "PCS monitor",
    "slug": "PCS-monitor",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./src/assets/images/playstore.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/splash-image.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "eas": {
        "projectId": "75eb8ba0-e467-4527-82ac-4d849843a952"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "PCSmonitor",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Allow PCS Monitor to use Face ID."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow PCS Monitor to use Face ID."
        }
      ]
    ]
  }
}
